{

    "manifest_version": 2,
    "name": "Tab Playback Device Selector",
    "version": "1.0",
  
    "description": "Firefox Browser Tab Playback Device Selector",
  
    "icons": {
      "24": "icons/tabplaybackds-24.png",
      "48": "icons/tabplaybackds-48.png"
    },
    
    "permissions": [
        "activeTab"
    ],

    "browser_action": {
        "default_icon": "icons/tabplaybackds-24.png.png",
        "default_title": "Tab Playback DS",
        "default_popup": "popup/popup.html"
      }

  }
  